<header class='top-page-header2'>
<%= link_to (image_tag 'logo.png' ,size: '140x140'),"/",class: :"logo-image"%>
</header>

<div class="form-main">
<div class="form">

<%= form_with model: @user, url: user_session_path, local: true do |f| %>
<form action="#">

  <div class="text-input">
    <%= f.label :email, "Email" %>
    <%= f.email_field :email, id:"user_email", autofocus: true, autocomplete: "email" %>
    <span class="separator"> </span><br />
  </div> 
  
    <div class="text-input">
    <%= f.label :password, "Password (6文字以上)" %>
    <%= f.password_field :password, id:"user_password", autocomplete: "new-password" %>
    <span class="separator"> 
  </div> 

  <div class="form-bottom">
    <input type="submit" id="submit" value="Submit"  />

  
  <a href= / class="top-src">Top Page</a>
    </div>
</form>  
  <% end %>
  

</div>
</div>



